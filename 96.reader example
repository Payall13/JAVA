import java.io.*;
public class readerexample
{
	public static void main(String args[])
	{
		try
		{
			Reader reader=new FileReader("output.txt");
			int data=reader.read();
			while(data != -1)
			{
				System.out.println((char )data);
				data=reader.read();
			}
			reader.close();
		}
		catch (Exception ex)
		{
			System.out.println(ex.getMessage());
		}
	}
}
