import java.io.*;
class read4
{
	public static void main(String args[])throws Exception
	{
		int b;
		FileInputStream fis=new FileInputStream("C://payal//write4.txt");
		BufferedInputStream bis= new BufferedInputStream(fis);
		while((b=bis.read())!=-1)
		{
		System.out.println((char)b);
		}
		bis.close();
		fis.close();
	}
}
