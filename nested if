import java.util.*;
class nestedif
{
	public static void main(String args[])
	{
		int n;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a number");
		n=sc.nextInt();
		if(n!=0)
		{
			if(n>0)
			{
			System.out.println("no is positive");
		    }
		else
		    {
			System.out.println("no is negative");
		     }
	    }
	    else
	    {
		System.out.println("no is zero");
	     }
	}
}
