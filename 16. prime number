// prime number
import java.util.*;
class prime1
{
	public static void main(String args[])
	{
		int n,i;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter n:");
		n=sc.nextInt();
		for(i=2;i<n;i++)
		{
			if(n%i==0)
				break;
		}
			if(n==i)
			System.out.println(n+" is prime number");
			else
			System.out.println(n+" is not prime number");
	}
}
