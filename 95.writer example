import java.io.*;
public class writerexample
{
	public static void main(String args[])
	{
		try
		{
			Writer w =new FileWriter("output.txt");
			String content="I Love my Country.";
			w.write(content);
			w.close();
			System.out.println("done");
		}
		catch (Exception e)
		{
			e.printStackTrace();
		}
	}
}
